	local script01a = Instance.new("TextButton")
	local script01frame = Instance.new("Frame")
	local loadineditor = Instance.new("TextButton")
	local execfiletab = Instance.new("TextButton")
	
	local sourced = source.Text
	local name = scriptname.Text
	
	script01a.Name = "0AD8MAS078ASGUAJSG98AGAYGHA"
	script01a.Parent = Hub
	script01a.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	script01a.BackgroundTransparency = 1.000
	script01a.Size = ab
	script01a.Font = Enum.Font.SourceSans
	script01a.Text = (name.. ".lua")
	script01a.TextColor3 = Color3.fromRGB(255, 255, 255)
	script01a.TextSize = 14.000
	script01a.TextXAlignment = Enum.TextXAlignment.Left
	script01a.BorderColor3 = Color3.fromRGB(98, 149, 181)
	script01frame.Name = "A0SD8MAZ0SMDZ7MASFYAUSU9GA87SGA"
	script01frame.Parent = script01a
	script01frame.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	script01frame.Position = UDim2.new(0.05982906, 0, 1, 0)
	script01frame.Size = UDim2.new(0, 94, 0, 34)
	script01frame.Visible = false
	loadineditor.Name = "0AS9D8ZMAS7MFMZ0AS9F8ASGHJA"
	loadineditor.Parent = script01frame
	loadineditor.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	loadineditor.BorderSizePixel = 0
	loadineditor.Size = UDim2.new(0, 94, 0, 17)
	loadineditor.Font = Enum.Font.SourceSans
	loadineditor.Text = "Load in Editor"
	loadineditor.TextColor3 = Color3.fromRGB(255, 255, 255)
	loadineditor.BorderColor3 = Color3.fromRGB(98, 149, 181)
	loadineditor.TextSize = 14.000
	execfiletab.Name = "S90DAMSDASD7ZMASFHFSA987DAS"
	execfiletab.Parent = script01frame
	execfiletab.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	execfiletab.BorderColor3 = Color3.fromRGB(98, 149, 181)
	execfiletab.BorderSizePixel = 0
	execfiletab.Position = UDim2.new(0, 0, 0.5, 0)
	execfiletab.Size = UDim2.new(0, 94, 0, 17)
	execfiletab.Font = Enum.Font.SourceSans
	execfiletab.Text = "Execute File"
	execfiletab.TextColor3 = Color3.fromRGB(255, 255, 255)
	execfiletab.TextSize = 14.000
	script01a.MouseButton1Click:Connect(function()
		loadstring(sourced)()
		scriptcount = scriptcount + 1
		local logged = xw:Clone()
		logged.MouseButton1Click:Connect(function()
			scriptlogcode.Text = sourced
		end)
	end)
	script01a.MouseButton2Click:Connect(function()
		script01frame.Visible = true
		wait(1)
		script01frame.MouseLeave:Connect(function()
			script01frame.Visible = false
		end)
		loadineditor.MouseButton1Click:Connect(function()
			textbox1.Text = sourced
		end)
		execfiletab.MouseButton1Click:Connect(function()
			loadstring(sourced)()
		end)
	end)
	loadineditor.MouseEnter:Connect(function()
		loadineditor.BackgroundColor3 = Color3.fromRGB(0, 54, 80)
		loadineditor.BorderSizePixel = 1
		loadineditor.BackgroundTransparency = 0.6
	end)
	loadineditor.MouseLeave:Connect(function()
		loadineditor.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		loadineditor.BorderSizePixel = 0
		loadineditor.BackgroundTransparency = 0
	end)
	execfiletab.MouseEnter:Connect(function()
		execfiletab.BackgroundColor3 = Color3.fromRGB(0, 54, 80)
		execfiletab.BorderSizePixel = 1
		execfiletab.BackgroundTransparency = 0.6
	end)
	
	execfiletab.MouseLeave:Connect(function()
		execfiletab.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		execfiletab.BackgroundTransparency = 0
		execfiletab.BorderSizePixel = 0
	end)
	script01a.MouseEnter:Connect(function()
		script01a.BackgroundColor3 = Color3.fromRGB(0, 54, 80)
		script01a.BorderSizePixel = 1
		script01a.BackgroundTransparency = 0.6
	end)

	script01a.MouseLeave:Connect(function()
		script01a.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
		script01a.BorderSizePixel = 0
		script01a.BackgroundTransparency = 1
	end)
	scriptsavetab.Visible = false
end)



